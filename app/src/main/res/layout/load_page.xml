<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".views.MainActivity"
    android:background="@drawable/f29f474840deb8dcdea76a113b543fbf"
    android:gravity="center"
    android:orientation="vertical"
    >

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/click"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:text="Longitude_longitude" />

        <com.google.android.material.textfield.TextInputEditText
            android:id="@+id/click_lattitude"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Lattitude"

            />

        <Button
            android:id="@+id/eventClick"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/rechercher"
            android:textColor="@color/black"
            app:layout_constraintTop_toBottomOf="@id/returnData"
             />
</LinearLayout>